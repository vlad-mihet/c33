{
  "version": 1,
  "importTypes": {
    "ap": {
      "description": "Accounts Payable invoices",
      "filename": "Accounts-Payable.xlsx",
      "sheet": "Sheet1",
      "collection": "accounts_payable",
      "uniqueKey": "APID",
      "mapping": {
        "APID": "apId",
        "Vendor": "vendor",
        "InvoiceDate": "invoiceDate",
        "DueDate": "dueDate",
        "Amount": "amount",
        "Currency": "currency",
        "Status": "status",
        "PaidDate": "paidDate",
        "Terms": "terms"
      },
      "indexes": [
        { "fields": { "apId": 1 }, "unique": true },
        { "fields": { "vendor": 1 } },
        { "fields": { "dueDate": 1 } }
      ]
    },
    "ar": {
      "description": "Accounts Receivable invoices",
      "filename": "Accounts-Receivable.xlsx",
      "sheet": "Sheet1",
      "collection": "accounts_receivable",
      "uniqueKey": "ARID",
      "mapping": {
        "ARID": "arId",
        "Customer": "customer",
        "InvoiceDate": "invoiceDate",
        "DueDate": "dueDate",
        "Amount": "amount",
        "Currency": "currency",
        "Status": "status",
        "ReceivedDate": "receivedDate",
        "Terms": "terms"
      },
      "indexes": [
        { "fields": { "arId": 1 }, "unique": true },
        { "fields": { "customer": 1 } },
        { "fields": { "dueDate": 1 } }
      ]
    },
    "gl": {
      "description": "General Ledger journal entries",
      "filename": "General-Ledger.xlsx",
      "sheet": "Sheet1",
      "collection": "general_ledger",
      "uniqueKey": "GLID",
      "mapping": {
        "GLID": "glId",
        "TxnDate": "txnDate",
        "AccountNumber": "accountNumber",
        "AccountName": "accountName",
        "Debit": "debit",
        "Credit": "credit",
        "Dept": "dept",
        "CostCenter": "costCenter",
        "Description": "description",
        "Currency": "currency"
      },
      "indexes": [
        { "fields": { "glId": 1 }, "unique": true },
        { "fields": { "txnDate": 1 } },
        { "fields": { "accountNumber": 1 } },
        { "fields": { "dept": 1 } }
      ]
    },
    "expenseClaims": {
      "description": "Employee expense claims",
      "filename": "Expense-Claims.xlsx",
      "sheet": "Sheet1",
      "collection": "expense_claims",
      "uniqueKey": "ClaimID",
      "mapping": {
        "ClaimID": "claimId",
        "EmployeeID": "employeeId",
        "SubmitDate": "submitDate",
        "Category": "category",
        "Description": "description",
        "Amount": "amount",
        "Currency": "currency",
        "Status": "status",
        "ApprovedBy": "approvedBy",
        "PayDate": "payDate"
      },
      "indexes": [
        { "fields": { "claimId": 1 }, "unique": true },
        { "fields": { "employeeId": 1 } },
        { "fields": { "submitDate": 1 } }
      ]
    },
    "budgetForecast": {
      "description": "Budget vs forecast by quarter and department",
      "filename": "Budget-Forecast.xlsx",
      "sheet": "Sheet1",
      "collection": "budget_forecast",
      "uniqueCompositeKey": ["FiscalYear", "Dept", "Quarter"],
      "mapping": {
        "FiscalYear": "fiscalYear",
        "Dept": "dept",
        "Quarter": "quarter",
        "BudgetUSD": "budgetUsd",
        "ForecastUSD": "forecastUsd",
        "ActualUSD": "actualUsd",
        "VarianceUSD": "varianceUsd",
        "Notes": "notes"
      },
      "indexes": [
        { "fields": { "fiscalYear": 1, "dept": 1, "quarter": 1 }, "unique": true },
        { "fields": { "dept": 1 } },
        { "fields": { "quarter": 1 } }
      ]
    }
  }
}
